{
  "name": "food-ordering-backend",
  "version": "1.0.0",
  "description": "Backend for Food Ordering Application",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --watch src --exec ts-node src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate",
    "seed": "mkdir -p scripts && ts-node scripts/seed.ts"
  },
  "keywords": [
    "node",
    "typescript",
    "express",
    "sequelize",
    "sqlite"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "morgan": "^1.10.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.1",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.1.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.2.5",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
